   &lt;circle cx=&quot;150&quot; cy=&quot;287&quot; r=&quot;100&quot; fill=&quot;white&quot; style=&quot;stroke:rgb(0,0,0);stroke-width:1;fill-opacity:0;stroke-dasharray: 10 5&quot; /&gt; 

  
  
